{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "globals": {
    "require": true,
    "module": true,
    "Promise": true,
    "process": true,
  },
  "plugins": [
    "react",
    "import",
    "jsx-a11y"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "no-console": 0,
    "import/no-unresolved": 0,
  }
}
